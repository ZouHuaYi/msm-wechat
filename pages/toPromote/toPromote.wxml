<view class="container">
	<view class="bgk"></view>
	<view class="header">
		<image class="main-back" src="../../assets/main_back.png"></image>
		<view class="h-top">
			<image class="avatar" src="../../assets/default.png"></image>
			<view class="h-content">
				<view class="h-title">{{newShowData.userNickname}}</view>
				<view class="h-bay">
					<view class="hb-item"><image class="icon" src="../../assets/taocan.png" mode="cover"></image><text class="tit">{{newShowData.pack}}</text></view>
					<view class="hb-item" wx:if="{{newShowData.gradeText}}"><image class="icon" src="../../assets/star.png" mode="cover"></image><text class="tit">{{newShowData.gradeText}}</text></view>
				</view>
				<view class="h-prom">推广: {{newShowData.pUserNickname}}</view>
				
			</view>
		</view>
		<button class="h-href" wx:if="{{newShowData.packageType==0}}"><text class="icon iconfont icon-huiyuanqiaguanli"></text>购买套餐<text class="icon iconfont icon-jiantou"></text></button>
		<button class="h-href" wx:if="{{newShowData.packageType!=0}}"><text class="icon iconfont icon-erweima"></text>推广二维码<text class="icon iconfont icon-jiantou"></text></button>
	</view>
	<view class="goto-hospital">
		<text class="tit">测试美容院</text>
		<view class="change">
			<text class="tip">切换</text>
			<text class="icon iconfont icon-qiehuan"></text>
		</view>
	</view>
	<view class="center-box">
		<view class="c-top">
			<text class="c-zh">账号余额</text>
			<text class="c-mon">￥0.00</text>
			<button class="c-btn" size="mini">￥提现</button>
		</view>
		<view class="c-free">
			<text class="cf-monney">冻结金额</text>
			<text class="cf-num">￥0.00</text>
		</view>
		<view class="c-bot">
			<view class="cb-item">
				<text class="cbi-mon">￥0.00</text>
				<text class="cbi-tit">退押收入</text>
			</view>
			<view class="cb-item">
				<text class="cbi-mon">￥10.00</text>
				<text class="cbi-tit">免费收入</text>
			</view>
			<view class="cb-item">
				<text class="cbi-mon">￥10.00</text>
				<text class="cbi-tit">重销收入</text>
			</view>
			<view class="cb-item">
				<text class="cbi-mon">￥0.00</text>
				<text class="cbi-tit">奖励收入</text>
			</view>
		</view>
	</view>
	<view class="footer">
		<block wx:for="{{navlist}}" wx:key="{{index}}">
				<view class="f-item">
					<image class="icon" mode="widthFix" src="{{item.icon}}"></image>
					<text class="tit">{{item.title}}</text>
				</view>
		</block>
	
	</view>
</view>
<view class="select-mak">
	<view class="mak-hidden"></view>
	<view class="select-box">
		<view class="select-content">
			<view class="select-list">
				<view class="select-item">
					<view class="s-item-title"><text>美容院</text></view>
				</view>
			</view>
		</view>
		<button class="add-news-btn" type="default">推广新医院</button>
	</view>
</view>